Proceso tabla_de_multiplicar
	
	definir num como real;
	definir res como real;
	definir i como real;
	escribir " ingrese el numero a multiplicar";
	leer num;
	Para i <- 1 Hasta 10 Con Paso 1 Hacer
		res <- i * num;
		escribir i,"x", num, "=", res ;
	FinPara
	
	
	
FinProceso


